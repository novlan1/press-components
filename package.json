  "scripts": {
    "build": "pnpm run build:elp && pnpm run build:tdesign && pnpm run build:shared",
    "build:elp": "pnpm run -C packages/element-plus build",
    "build:shared": "pnpm run -C packages/shared build",
    "build:tdesign": "pnpm run -C packages/tdesign-vue-next build",
    "docs:build": "pnpm run -C docs docs:build && pnpm run docs:build:elp && pnpm run docs:build:tdesign",
    "docs:build:elp": "pnpm run -C packages/element-plus docs:build",
    "docs:build:igame": "VITE_DOCS_PUBLISH_PATH=/pmd-mobile.pmd-h5.press-components.press/ VITE_APP_PUBLIC_PATH=https://image-1251917893.file.myqcloud.com npm run docs:build",
    "docs:build:tdesign": "pnpm run -C packages/tdesign-vue-next docs:build",
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:dev:tdesign": "pnpm run -C packages/tdesign-vue-next dev",
    "release": "node scripts/release.ts"
  },
  "dependencies": {
    "@types/node": "^18.0.3",
    "chalk": "^4.1.2",
    "ebus-light": "^0.0.2",
    "execa": "^5.1.0",
    "fs-extra": "^10.1.0",
    "press-next": "^1.0.27",
    "press-plus": "^1.7.145",
    "press-ui": "^3.0.3",
    "share-light": "^0.0.5",
    "t-comm": "^1.5.12",
    "vite": "6.2.6",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-jsx": "3.1.0",
